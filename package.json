{
  "name": "prisma-graphql-type-decimal",
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "GraphQL type for Prisma's Decimal scalar, wrapper around decimal.js",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/prisma-graphql-type-decimal.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/prisma-graphql-type-decimal/issues"
  },
  "homepage": "https://github.com/unlight/prisma-graphql-type-decimal#readme",
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:m": "node node_modules/@stryker-mutator/core/bin/stryker.js run",
    "test:r": "node -r ts-node/register node_modules/mocha/bin/mocha src/**/*.spec.ts",
    "test:cov": "c8 --reporter text --exclude \"**/*.spec.ts\" --exclude \"**/testing/**\" npm run test:r",
    "test:w": "npm run test:r -- --watch --watch-files src",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "format:src": "prettier src --write",
    "commit_lint": "sh Taskfile commit_lint",
    "commit": "cz",
    "build": "sh Taskfile buildMicrobundle4",
    "test:d": "ndb -r ts-node/register/transpile-only node_modules/mocha/bin/mocha --no-timeouts --watch-files src/**/*.ts --watch src/**/*.spec.ts"
  },
  "peerDependencies": {
    "@prisma/client": "4.12 - 6",
    "graphql": "16"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.24.0",
    "@prisma/client": "^5.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stryker-mutator/api": "^8.7.1",
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/mocha-runner": "^8.7.1",
    "@swc/core": "^1.11.16",
    "@swc/helpers": "^0.5.15",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "c8": "^10.1.3",
    "class-transformer": "^0.5.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.4.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-perfectionist": "^4.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-wix-editor": "^3.3.0",
    "expect": "^29.7.0",
    "globals": "^16.0.0",
    "graphql": "^16.6.0",
    "microbundle": "^0.15.1",
    "mocha": "^11.1.0",
    "precise-commits": "^1.0.2",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "reflect-metadata": "^0.2.2",
    "semantic-release": "^24.2.3",
    "simplytyped": "^3.3.0",
    "stryker-cli": "^1.0.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  }
}
