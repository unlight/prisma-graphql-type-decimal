{
  "name": "prisma-graphql-type-decimal",
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "GraphQL type for Prisma's Decimal scalar, wrapper around decimal.js",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/prisma-graphql-type-decimal.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/prisma-graphql-type-decimal/issues"
  },
  "homepage": "https://github.com/unlight/prisma-graphql-type-decimal#readme",
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "node -r ts-node/register node_modules/mocha/bin/mocha src/**/*.spec.ts",
    "test:cov": "c8 --reporter text --exclude \"**/*.spec.ts\" --exclude \"**/testing/**\" npm run test:r",
    "test:w": "npm run test:r -- --watch --watch-files src",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "format:src": "prettier src --write",
    "commit_lint": "sh Taskfile commit_lint",
    "commit": "cz",
    "build": "sh Taskfile build_pkgroll",
    "test:d": "ndb -r ts-node/register/transpile-only node_modules/mocha/bin/mocha --no-timeouts --watch-files src/**/*.ts --watch src/**/*.spec.ts"
  },
  "peerDependencies": {
    "@prisma/client": "3 4",
    "graphql": "16"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@prisma/client": "^4.8.1",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@swc/core": "^1.3.26",
    "@swc/helpers": "^0.4.14",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "c8": "^7.12.0",
    "class-transformer": "^0.5.1",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-regexp": "^1.12.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-total-functions": "^6.0.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-plugin-wix-editor": "^3.3.0",
    "expect": "^29.3.1",
    "git-branch-is": "^4.0.0",
    "graphql": "^16.6.0",
    "mocha": "^10.2.0",
    "precise-commits": "^1.0.2",
    "prettier": "^2.8.2",
    "prisma": "^4.8.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "semantic-release": "^20.0.2",
    "simplytyped": "^3.3.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "watchexec-bin": "^1.0.0"
  }
}
