{
  "name": "prisma-graphql-type-decimal",
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "GraphQL type for Prisma's Decimal scalar, wrapper around decimal.js",
  "module": "",
  "types": "",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/prisma-graphql-type-decimal.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/prisma-graphql-type-decimal/issues"
  },
  "homepage": "https://github.com/unlight/prisma-graphql-type-decimal#readme",
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:m": "node node_modules/@stryker-mutator/core/bin/stryker.js run",
    "test:r": "node -r ts-node/register node_modules/mocha/bin/mocha src/**/*.spec.ts",
    "test:cov": "c8 --reporter text --exclude \"**/*.spec.ts\" --exclude \"**/testing/**\" npm run test:r",
    "test:w": "npm run test:r -- --watch --watch-files src",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "format:src": "prettier src --write",
    "commit": "cz",
    "build": "sh Taskfile buildMicrobundle5",
    "test:d": "ndb -r ts-node/register/transpile-only node_modules/mocha/bin/mocha --no-timeouts --watch-files src/**/*.ts --watch src/**/*.spec.ts"
  },
  "peerDependencies": {
    "@prisma/client": "7",
    "graphql": "16"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^9.39.2",
    "@prisma/client": "^7.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stryker-mutator/api": "^9.5.0",
    "@stryker-mutator/core": "^9.5.0",
    "@stryker-mutator/mocha-runner": "^9.5.0",
    "@swc/core": "^1.15.11",
    "@swc/helpers": "^0.5.18",
    "@types/graphql": "^14.5.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "c8": "^10.1.3",
    "class-transformer": "^0.5.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-perfectionist": "^5.4.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-wix-editor": "^3.3.0",
    "expect": "^30.2.0",
    "globals": "^17.2.0",
    "graphql": "^16.12.0",
    "microbundle": "^0.15.1",
    "mocha": "^11.7.5",
    "prettier": "^3.8.1",
    "prisma": "^7.3.0",
    "reflect-metadata": "^0.2.2",
    "semantic-release": "^25.0.2",
    "simplytyped": "^3.3.0",
    "stryker-cli": "^1.1.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
